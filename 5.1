#include <stdio.h>
#include <string.h>
#include <sys/stat.h>
#include <sys/types.h>


int main(int argc, char *argv[])
{
int res = 5;
    if (argc > 3){
     printf("Too many args!\n");
      return 0;
}
  if (!strcmp(argv[1], "-c")){
    printf("Creating directory: %s\n", argv[2]);
    res = mkdir(argv[2], 0777);
      if (res == 0)
       printf("Success!\n");
      else if (res == -1)
       printf("Fail!\n");
}
      else if (!strcmp(argv[1], "-d")) {
       printf("Removing directory: %s\n", argv[2]);
        res = remove(argv[2]);
          if (res == 0)
            printf("Success!\n");
          else if (res == -1)
            printf("Fail!\n");
  }
    else {
      printf("Incorrect command argument!\n");
}
  return 0;
}
